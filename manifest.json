{

  "manifest_version": 2,

  "name": "Songly",
  "description": "Display lyrics to your favorite spotify songs.",
  "version": "1.0",
  "homepage_url": "http://www.mozillazine.org/",

  "icons": {
    "48": "icons/icon48.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/icon16.png"
  },

  "permissions": [
    "activeTab"
  ]

}
